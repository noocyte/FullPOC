@model IEnumerable<RiskyWeb.Models.AnalysisView>
@{
    ViewBag.Title = "Index";
}
<h2>
    Index</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Title
        </th>
        <th>
            Description
        </th>
        <th>
            Created
        </th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Created)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.AnalysisRowKey, partKey = item.AnalysisPartKey }) |
                @Html.ActionLink("Details", "Details", new { id = item.AnalysisRowKey, partKey = item.AnalysisPartKey }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.AnalysisRowKey, partKey = item.AnalysisPartKey })
            </td>
        </tr>
    }
</table>
